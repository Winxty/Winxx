local Fluent = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()
local SaveManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/SaveManager.lua"))()
local InterfaceManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/InterfaceManager.lua"))()

game:GetService("StarterGui"):SetCore("SendNotification", {
    Title = "Sypherion",
    Text = "Loading the script",
    Icon = "",
    Duration = 5
})




local randoms = {
    [1] = "ShoutOut To All UsersðŸ”¥ðŸ”¥",
    [2] = "You're poopy",
    [3] = "Sigma?"
}

local rndtlt = math.random(1, #randoms)
local rnd = randoms[rndtlt]

local Window = Fluent:CreateWindow({
    Title = "Sypherion | Winxx | Blox Fruits "  ,
    SubTitle = rnd,
    TabWidth = 160,
    Size = UDim2.fromOffset(580, 460),
    Acrylic = true,
    Theme = "Dark",
    MinimizeKey = Enum.KeyCode.LeftControl
})


local Tabs = {
    Main = Window:AddTab({ Title = "Main", Icon = "" }),
    Player = Window:AddTab({ Title = "Player", Icon = "user" }),
    Farm = Window:AddTab({ Title = "Farm", Icon = "leaf" }),
    Misc = Window:AddTab({ Title = "Misc", Icon = "shuffle" }),
    Settings = Window:AddTab({ Title = "Settings", Icon = "settings" })
}

local Options = Fluent.Options

do


    Tabs.Main:AddParagraph({
        Title = "Welcome to Sypherion",
        Content = "Didnt add alot yet cus i stressed this out, but i will add more features soon."
    })


    local TweenService = game:GetService("TweenService")

    function tween(targetX, targetY, targetZ, part)
        local suppart = Instance.new("Part")
        suppart.Size = Vector3.new(4, 1, 4)
        suppart.Position = game.Players.LocalPlayer.Character.HumanoidRootPart.Position - Vector3.new(0, 3, 0)
        suppart.Anchored = true
        suppart.CanCollide = false
        suppart.Parent = game.Workspace
    
        part = part or game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart")
    
        local targetPosition = Vector3.new(targetX, targetY, targetZ)
    
        local tweenInfo = TweenInfo.new(
            10,
            Enum.EasingStyle.Linear,
            Enum.EasingDirection.Out,
            0,
            false,
            0
        )
    
        local goal = {Position = targetPosition}
    
        local tween = TweenService:Create(part, tweenInfo, goal)
    
        tween:Play()
    
        local supportGoal = {Position = part.Position - Vector3.new(0, 3, 0)}
        local supportTween = TweenService:Create(suppart, tweenInfo, supportGoal)
        supportTween:Play()
    
        tween.Completed:Connect(function()
            print("Tween completed!")
            suppart:Destroy()
        end)
    end
    
    
    
    


local function noclip()
    local char = game.Players.LocalPlayer.Character
        char.LowerTorso.CanCollide = false
        char.UpperTorso.CanCollide = false
        char.HumanoidRootPart.CanCollide = false
end

local function noclipoff()
    local char = game.Players.LocalPlayer.Character
        char.LowerTorso.CanCollide = true
        char.UpperTorso.CanCollide = true
        char.HumanoidRootPart.CanCollide = true
end

local function reset()

local player = game.Players.LocalPlayer


local humanoid = player.Character and player.Character:FindFirstChild("Humanoid")
if humanoid then
    humanoid.Health = 0
end

    
end


local trollfarm = false
local kickcounter = 0

    

    local Toggle = Tabs.Farm:AddToggle("MyToggle", 
{
    Title = "Farm Level", 
    Description = "",
    Default = false,
    Callback = function(state)
	if state then
        trollfarm = true

        if trollfarm then
            noclip()
            tween(5409.5107421875, 81.02201843261719, 4179.6962890625  )
        end

        if game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.Position == Vector3.new(5409.5107421875, 81.02201843261719, 4179.6962890625) then
            noclipoff()
            wait(1)
            reset()
        end

        wait(5)




        while trollfarm do
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(5348.283203125, 39.3489990234375, 3953.2548828125)


            for i, v in pairs(game.Workspace.Enemies:GetChildren()) do
                if v.Name == "Galley Pirate" then
                    
                    v.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame - Vector3.new(0, 10, 0)
                end
            end
            
            kickcounter = kickcounter + 1
            if kickcounter == 100 then
                game.Players.LocalPlayer:Kick("You have been banned for exploiting.")
            end
            wait(0.2)
        end
        
        



	else



     end
    end 
})





Tabs.Misc:AddButton({
    Title = "Fruit Rain",
    Description = "",
    Callback = function()
        game.Players.LocalPlayer:Kick("You have been banned for exploiting.")
    end
})



Tabs.Misc:AddButton({
    Title = "Kill Sypherion",
    Description = "",
    Callback = function()
        Fluent:Destroy()
    end
})







SaveManager:SetLibrary(Fluent)
InterfaceManager:SetLibrary(Fluent)


SaveManager:IgnoreThemeSettings()


SaveManager:SetIgnoreIndexes({})


InterfaceManager:SetFolder("FluentScriptHub")
SaveManager:SetFolder("FluentScriptHub/specific-game")

InterfaceManager:BuildInterfaceSection(Tabs.Settings)
SaveManager:BuildConfigSection(Tabs.Settings)


Window:SelectTab(1)

Fluent:Notify({
    Title = "Fluent",
    Content = "The script has been loaded.",
    Duration = 8
})


SaveManager:LoadAutoloadConfig()
end
