


local Fluent = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()
local SaveManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/SaveManager.lua"))()
local InterfaceManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/InterfaceManager.lua"))()

local Window = Fluent:CreateWindow({
    Title = "Sypherion | Key-System",
    SubTitle = "Sorry For not having Luarmor",
    TabWidth = 160,
    Size = UDim2.fromOffset(580, 460),
    Acrylic = true, -- The blur may be detectable, setting this to false disables blur entirely
    Theme = "Dark",
    MinimizeKey = Enum.KeyCode.LeftControl -- Used when theres no MinimizeKeybind
})


local Tabs = {
    Main = Window:AddTab({ Title = "Main", Icon = "" }),
}

local Options = Fluent.Options

do

    Tabs.Main:AddParagraph({
        Title = "Info",
        Content = "Sorry for this key-system\n its the best i can do for now\ni cant really afford luarmor and allat\nIm still a godamn child and i dont have a job so sorry ü•∞üôè"
    })




    local Input = Tabs.Main:AddInput("Input", {
        Title = "Enter Key",
        Default = "",
        Placeholder = "Enter Key...",
        Numeric = false,
        Finished = true,
        Callback = function(key)
    
            local validkeystring = "–Ö—É—Ä–Ω—î—è—ñ–æ–∏"
    

            if string.find(key, validkeystring) then
                print("Access granted")
                Fluent:Destroy()
            else
                game.Players.LocalPlayer:Kick("Invalid key! \nGet key in Discord \nhttps://dsc.gg/ratware\n")
                setclipboard("https://dsc.gg/ratware")
                Fluent:Destroy()
            end
    
        end
    })


Tabs.Main:AddButton({
    Title = "Get Key",
    Description = "In discord until I can afford Luarmor :c",
    Callback = function()
        local HttpService = game:GetService("HttpService")
        local discordInvite = "https://discord.com/invite/4ku99RaHf4"

        local function handleInvite()
            if HttpService.HttpEnabled then
                local success, response = pcall(function()
                    return HttpService:GetAsync(discordInvite)
                end)

                if success then
                    Fluent:Notify({
                        Title = "Sypherion",
                        Content = "Successfully invited to Discord. Check your Discord.",
                        Duration = 8
                    })
                else
                    Fluent:Notify({
                        Title = "Sypherion",
                        Content = "Failed to retrieve the invite. Please try again later.",
                        Duration = 8
                    })
                end
            else
                Fluent:Notify({
                    Title = "Sypherion",
                    Content = "Invite link copied to clipboard.",
                    Duration = 8
                })
                setclipboard(discordInvite)
            end
        end

        handleInvite()
    end
})



Fluent:Notify({
    Title = "Sypherion",
    Content = "Key-System",
    Duration = 8
})

Window:SelectTab(1)

end
